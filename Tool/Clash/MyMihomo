# é‡å†™è§„åˆ™ (ä» Loon [Rewrite] è½¬æ¢)
rewrites:
  - type: regex-replace
    from: '^(http|https):\/\/(www.)?(g|google)\.cn'
    to: 'https://www.google.com'
  - type: regex-replace
    from: '^https?://(.*?[.])?(?:g|google).cn'
    to: 'https://$1google.com'
  - type: regex-replace
    from: '^(http|https):\/\/(ditu|maps).google\.cn'
    to: 'https://maps.google.com'
  - type: regex-replace
    from: '^http:\/\/t\.cn'
    to: 'http://sinaurl.cn'
  - type: regex-replace
    from: '^(http|https):\/\/nga\.178\.com'
    to: 'https://ngabbs.com'
  - type: regex-replace
    from: '^(http|https):\/\/bbs\.nga\.cn'
    to: 'https://ngabbs.com'
  - type: regex-replace
    from: '^https:\/\/zh.(m.)?wikipedia.org/zh(-\w*)?(?=/)'
    to: 'https://www.wikiwand.com/zh$2'
  - type: regex-replace
    from: '^https:\/\/(\w*).(m.)?wikipedia.org/wiki'
    to: 'https://www.wikiwand.com/$1'
  - type: regex-replace
    from: '^https?://union.click.jd.com/jda?'
    to: 'http://union.click.jd.com/jda?adblock='
